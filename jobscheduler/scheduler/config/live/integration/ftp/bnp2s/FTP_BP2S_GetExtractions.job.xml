    </environment>
	<script language="shell">
	<![CDATA[
	@echo off
	set mydate=%date:~6,4%%date:~3,2%%date:~0,2%

	  IF "%SCHEDULER_PARAM_DATE%" == "AAAAMMJJ" GOTO CONFIG_DATE_AUTO
	  set mydate=%SCHEDULER_PARAM_DATE%

	  :CONFIG_DATE_AUTO

  	  MKDIR "%OUTPUT_FILE_PATH%\RETOUR_%mydate%_PROD"
	  MKDIR "%OUTPUT_FILE_PATH2%\RETOUR\Traite_%mydate%_PROD\"
	  MKDIR "%OUTPUT_FILE_PATH2%\RETOUR\NonTraite_%mydate%_PROD\"
	  	  
	  %SCRIPT_PATH%\Winscp.exe /console /log=%LOG% /script=%SCRIPT_PATH%\%SCRIPT% /parameter %FILE% "%OUTPUT_FILE_PATH%\RETOUR_%mydate%_PROD"

rem : ne plus filtrer le plus gros fichier	  
rem	  rem mettre les swift unitaire dans le repertoire principal (tous sauf le plus gros)
rem      echo off
rem	  for /f "delims=" %%X in ('dir /os /b "%OUTPUT_FILE_PATH%\RETOUR_%mydate%_PROD\%FILE%"') do set allswift=%%X
rem	  for %%A in ( "%OUTPUT_FILE_PATH%\RETOUR_%mydate%_PROD\%allswift%") do set allswiftsize=%%~zA
rem	  
rem	  if [%allswift%] == [] GOTO CONT
rem	  
rem	  echo Fichier allswift est %allswift% d une taille %allswiftsize%
rem	  if %allswiftsize% LSS 800000 (
rem	      echo SERA TRAITE : Fichier allswift est %allswift% d une taille %allswiftsize% 
rem	  ) ELSE (
rem	      echo NON TRAITE : Fichier allswift est %allswift% d une taille %allswiftsize% 
rem          move "%OUTPUT_FILE_PATH%\RETOUR_%mydate%_PROD\%allswift%" %OUTPUT_FILE_PATH2%\RETOUR\NonTraite_%mydate%_PROD\%allswift%_ALL		 
rem	  )
	  
	  :CONT
	  
      for /f %%A in ( 'dir /b/a-d "%OUTPUT_FILE_PATH%\RETOUR_%mydate%_PROD\*" ^| find /v /c "::"') do set FileNb=%%A
	  echo OK sur la date %date% pour %FileNb% fichiers 

	  if [%FileNb%] == [] GOTO end
	 if %FileNb% EQU 0 GOTO end
	  
 
	  xcopy "%OUTPUT_FILE_PATH%\RETOUR_%mydate%_PROD" "%OUTPUT_FILE_PATH2%\RETOUR\Traite_%mydate%_PROD\" /E /C /I /Y	  
	  
	  rem copy sur SCD	  
	  move "%OUTPUT_FILE_PATH%\RETOUR_%mydate%_PROD\%FILE%" %OUTPUT_FILE_PATH2%\
	  
	  IF errorlevel 1 GOTO error

		echo Success
		echo OK sur la date %date% pour %FileNb% fichiers 
		
		goto end

		:error
		echo Error!

		:end
    ]]>	
	</script>
