<?xml version="1.0" encoding="ISO-8859-1"?>

<job title="Récupération des NAV_FILES.ZIP chez BP2S" order="no">
    <description>
        <![CDATA[
Connection et get du fichier *.NAV_FILES*.zip
        ]]>
    </description>
	<!-- les parametres sont modifiables dans l interface web du jobsheduler avec "start Task parametrized" 
	     les variables SCHEDULER_PARAM_XXXX permettent d accéder aux valeurs des paramètres
	-->
	<!-- les variables d environnement permettent de rendre plus clair la config , on peut utiliser des variables d env système -->
	<environment>
		<variable name="LOG"
				  value="&quot;\\vill1\Partage\,FGA Systèmes\PRODUCTION\FTP\LOG\!Y!M!D-!T-!S-NAVFiles-log.txt&quot;"/>		  
		<variable name="SCRIPT_PATH"
				  value="&quot;C:\FGA_SOFT_INTEGRATION\BATCH\FTP&quot;"/>		  	
		<variable name="SCRIPT"
				  value="&quot;script_BP2S_SFTP_Get_Extractions.txt&quot;"/>
		<variable name="OUTPUT"
				  value="&quot;\\vill1\Partage\SCD_FILES_PROD\IN\VALO\BP2S&quot;"/>
		<variable name="FILE"
				  value="&quot;*.NAV_FILES_*.zip&quot;"/>

    </environment>
	<script language="shell">
	<![CDATA[
	@echo off
	set mydate=%date:~6,4%%date:~3,2%%date:~0,2%
	
	 %SCRIPT_PATH%\Winscp.exe /console /log=%LOG% /script=%SCRIPT_PATH%\%SCRIPT% /parameter %FILE% %OUTPUT%
      
	  IF errorlevel 1 GOTO error

		echo Success		
		MKDIR %OUTPUT%\Unzip
	
		rem dezip et mettre les fichiers dans les repertoires adéquates
		"C:\Program Files (x86)\7-Zip\7z.exe" e %OUTPUT%\%FILE% -o%OUTPUT%\Unzip -y

		rem Archivage des fichiers traites		
		move %OUTPUT%\%FILE% %OUTPUT%\Archive\ 
	
		rem un repertoire pour mettre les fichiers traites ds SCD
		MKDIR "\\vill1\Partage\SCD_FILES_PROD\IN\SWIFT\RETOUR\Traite_%mydate%_PROD"
								
		rem deplacements des fichiers dans les rep. SCD
		xcopy %OUTPUT%\Unzip\Standard_Financial_Statements_-_CSV.*.csv "\\vill1\Partage\SCD_FILES_PROD\IN\SWIFT\RETOUR\Traite_%mydate%_PROD"
		xcopy %OUTPUT%\Unzip\Standard_EOD_Net_Positions_-_CSV.*.csv "\\vill1\Partage\SCD_FILES_PROD\IN\SWIFT\RETOUR\Traite_%mydate%_PROD"		
		move %OUTPUT%\Unzip\Standard_Financial_Statements_-_CSV.*.csv \\vill1\Partage\SCD_FILES_PROD\IN\UBIX\BP2S\
		move %OUTPUT%\Unzip\Standard_EOD_Net_Positions_-_CSV.*.csv \\vill1\Partage\SCD_FILES_PROD\IN\UBIX\BP2S\
		
		echo "Fichier CSTA_ALL001_*.csv"		
		rem ne prendre que le fichier le plus important en taille
		echo off  
		for /f "delims=" %%x in ('dir /os /b %OUTPUT%\Unzip\CSTA_ALL001_*.csv') do set treso=%%x
		echo %treso%				
		xcopy %OUTPUT%\Unzip\%treso% "\\vill1\Partage\SCD_FILES_PROD\IN\SWIFT\RETOUR\Traite_%mydate%_PROD"
		move %OUTPUT%\Unzip\%treso% \\vill1\Partage\SCD_FILES_PROD\IN\RECO\BP2S\CASH\

		rem archivage du "pack vl" 
		xcopy %OUTPUT%\Unzip\*_FONDS_*_Standard_PAR_-_Historique_des_frais_de_gestion*.csv %OUTPUT%\INV_UNZIP
		xcopy %OUTPUT%\Unzip\*_FONDS_*_Standard_PAR_-_Dernieres_valeurs_liquidatives*.csv %OUTPUT%\INV_UNZIP
		xcopy %OUTPUT%\Unzip\*_FONDS_*_Standard_PAR_-_Inventaire*.csv %OUTPUT%\INV_UNZIP
		rem move %OUTPUT%\Unzip\*_FONDS_*_Standard_PAR_-_Historique_des_frais_de_gestion*.csv \\vill1\Partage\SCD_FILES_PROD\IN\VALO\BP2S\
		rem move %OUTPUT%\Unzip\*_FONDS_*_Standard_PAR_-_Dernieres_valeurs_liquidatives*.csv \\vill1\Partage\SCD_FILES_PROD\IN\VALO\BP2S\
		rem move %OUTPUT%\Unzip\*_FONDS_*_Standard_PAR_-_Inventaire*.csv \\vill1\Partage\SCD_FILES_PROD\IN\VALO\BP2S\		
				
		rem deplacer des fichiers mouvements dans Pack VL
		xcopy %OUTPUT%\Unzip\*_Standard_PAR_-_Mouvements*.csv %OUTPUT%\INV_UNZIP
		xcopy %OUTPUT%\Unzip\*_Standard_PAR_-_Taux_de_change*.csv %OUTPUT%\INV_UNZIP
		rem pas de move car non traite dans SCD
				
		rem tout se qui reste est mis dans le repertoire swift
		MKDIR "\\vill1\Partage\SCD_FILES_PROD\IN\SWIFT\RETOUR\NonTraite_%mydate%_PROD"
		move %OUTPUT%\Unzip\* \\vill1\Partage\SCD_FILES_PROD\IN\SWIFT\RETOUR\NonTraite_%mydate%_PROD\
		
		goto end

		:error
		echo Error!

		:end
    ]]>	
	</script>
